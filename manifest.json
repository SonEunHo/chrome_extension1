{
    "name": "srt chrome extension",
    "version": "0.1",
    "description": "nano",
    "permissions": ["storage", "tabs"],
    "background": {
        "page": "background.html"
    },
    "content_scripts": [
        {
          "matches": ["https://etk.srail.co.kr/hpg/hra/01/selectScheduleList.do*", "http://etk.srail.co.kr/hpg/hra/01/selectScheduleList.do*"],
          "css": ["srtCss.css"],
          "js": ["jquery-3.3.1.min.js", "srtScript.js"],
          "run_at": "document_end",
          "all_frames": true
        }
    ],
    "web_accessible_resources": [
        "success.wav"
    ],
    "manifest_version": 2
}